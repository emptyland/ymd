ForLoopTest = @{
	benchmark : 3000000,

	test_foreach_benchmark : func (self) {
		for var i in range(0, self.benchmark) {
		}
	},

	test_foreach_benchmark_slow : func (self) {
		var func range(i, end) {
			return func () {
				if i > end {
					return nil
				}
				i = i + 1
				return i
			}
		}
		for var i in range(0, self.benchmark) {
		}
	},

	test_forstep_benchmark : func (self) {
		var i
		for let i = 0, self.benchmark {
		}
	}
}
