func generate(a, b, c) {
	return func [a] (b, c) {
		a = a + 1
		return a
	}
}
var generator = generate(1, 2, 3)
print(generator())
print(generator())
print(generator())
print(generator())

func buildNumber(a) {
	return func [a] () {
		a = a ^ 2
		return a
	}
}
var fn = buildNumber(2)
print(fn())
print(fn())
print(fn())
print(fn())
