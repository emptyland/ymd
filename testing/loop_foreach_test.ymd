print("==== simple:")
for var i: range(1,3) {
	print(i)
}

print("==== break:")
for i: range(0, 100) {
	print(">", i)
	if i >= 6 {
		break
	}
}

print("==== nested:")
for i: range(0, 1) {
	for var j: range(2, 3) {
		print(i, j)
	}
}

print("==== priv")
iter = range(0,100)
for {
	i = iter()
	print(i)
	if i > 6 {
		break
	}
	continue
	print("Noreached!")
}

print("==== array")
arr = {2,3,1,5,7,0}
for i: range(arr) {
	print("[]", i)
}

